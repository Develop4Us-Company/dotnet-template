{
  "ConnectionStrings": {
    "DatabaseConnection": "<<SET_SQLSERVER_DATABASE_CONNECTION_STRING>>",
    "HangfireConnection": "<<SET_HANGFIRE_SQLSERVER_CONNECTION_STRING>>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "Auth0": {
    "Authority": "<<SET_AUTH0_AUTHORITY>>",
    "ClientId": "<<SET_AUTH0_CLIENT_ID>>",
    "Audience": "<<SET_AUTH0_AUDIENCE>>"
  },
  "SystemAdminUser": {
    "Name": "<<SET_SYSTEM_ADMIN_NAME>>",
    "Email": "<<SET_SYSTEM_ADMIN_EMAIL>>"
  },
  "Cors": {
    "AllowedOrigins": [
      "<<SET_ALLOWED_CORS_ORIGINS>>"
    ]
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowSeconds": 60,
    "QueueLimit": 10
  },
  "AllowedHosts": "<<SET_ALLOWED_HOSTS>>",
  "SendEmail": {
    "ApiKey": "<<SET_SENDGRID_API_KEY>>",
    "FromEmailAddress": "<<SET_SENDGRID_FROM_EMAIL>>",
    "FromName": "<<SET_SENDGRID_FROM_NAME>>"
  },
  "AI": {
    "Endpoint": "<<SET_GITHUB_AI_ENDPOINT>>",
    "Token": "<<SET_GITHUB_AI_TOKEN>>"
  }
}
