{
  "ConnectionStrings": {
    "DatabaseConnection": "Server=localhost,1433;Database=AppProject;User Id=sa;Password=yourStrong(!)Password;TrustServerCertificate=True;",
    "HangfireConnection": "Server=localhost,1433;Database=AppProject;User Id=sa;Password=yourStrong(!)Password;TrustServerCertificate=True;"
  },
  "Auth0": {
    "Authority": "<<SET_AUTH0_AUTHORITY>>",
    "ClientId": "<<SET_AUTH0_CLIENT_ID>>",
    "Audience": "<<SET_AUTH0_AUDIENCE>>"
  },
  "SystemAdminUser": {
    "Name": "<<SET_SYSTEM_ADMIN_NAME>>",
    "Email": "<<SET_SYSTEM_ADMIN_EMAIL>>"
  },
  "Cors": {
    "AllowedOrigins": [
      "<<SET_ALLOWED_CORS_ORIGINS>>"
    ]
  },
  "AllowedHosts": "localhost;127.0.0.1",
  "SendEmail": {
    "ApiKey": "<<SET_SENDGRID_API_KEY>>",
    "FromEmailAddress": "<<SET_SENDGRID_FROM_EMAIL>>",
    "FromName": "<<SET_SENDGRID_FROM_NAME>>"
  },
  "AI": {
    "Endpoint": "<<SET_GITHUB_AI_ENDPOINT>>",
    "Token": "<<SET_GITHUB_AI_TOKEN>>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/logs.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] ({UserEmail}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
